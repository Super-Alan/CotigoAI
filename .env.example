# Database
DATABASE_URL="postgresql://user:password@localhost:5432/cogito_ai"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"

# AI Models
DEEPSEEK_API_KEY="your-deepseek-api-key"
DEEPSEEK_API_URL="https://bailian.console.aliyun.com/api/v1"

QWEN_API_KEY="your-qwen-api-key"
QWEN_API_URL="https://bailian.console.aliyun.com/api/v1"

# Active AI Model (deepseek-v3.1 | qwen3-max)
ACTIVE_AI_MODEL="deepseek-v3.1"

# AI Request Configuration
# Timeout for AI API requests in milliseconds (default: 60000 = 60s)
# Increase this value if you experience timeout errors on Vercel
AI_REQUEST_TIMEOUT="60000"

# Redis (optional, for caching)
REDIS_URL="redis://localhost:6379"

# Volcengine Doubao ASR (Voice Input)
VOLCENGINE_APP_ID="your-volcengine-app-id"
VOLCENGINE_ACCESS_TOKEN="your-volcengine-access-token"
VOLCENGINE_CLUSTER="volcengine_streaming_common"

# App Configuration
NODE_ENV="development"
